<table class="table table-striped table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Status</th>
      <th scope="col">Comments</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
      {{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.status}}</td>
      <td>{{this.comments}}</td>
      <td>
          <a href="/edituser/{{this.id}}" type="button" class="btn btn-dark btn-sm"><i class="bi bi-pen-fill"></i> Edit</a>
          <a href="/{{this.id}}" type="button" class="btn btn-dark btn-sm"><i class="bi bi-trash-fill"></i> Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>